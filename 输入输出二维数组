#include <stdio.h>

int main()
{
    int x,y,a[10][2];
    for(x=0;x<=9;x++)
    {
        for(y=0;y<=1;y++)
        {
            scanf("%d",&a[x][y]);
        }
    }
    for(x=0;x<=9;x++)
    {
        for(y=0;y<=1;y++)
        {
            printf("%d\t",a[x][y]);
        }
    }
    printf("\n");
}



    

    
    

