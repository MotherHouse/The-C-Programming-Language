#include <stdio.h>

int main()
{
    int number;
    number=100;
    int x,y;printf("please input you number:");
    scanf("%d",&x);

    
     for(y=1;y<=5;y++)
         
    {
                if(x<number)
        
            printf("you input number is too small");
        
        
         else if (x>number)
    
        printf("you input number is too big");
        
    
    
     else if (x=number)
    
        printf("your input number is right");
    }
}

    
   
    

