#include <stdio.h>

int main()
{
    int number[5]={4,6,9,3,1};
    int i,j;
    int temp;
    for ( i=0; i<5; i++)
    {
        for (j=0;j<5;j++)
        
            if (number[j]>number[j+1])
            {
            temp=number[j];
            number[j]=number[j+1];
            number[j+1]=temp;
            }
    }
    
    for (i=0; i<5; i++)
    {
         printf("%d\t",number[i]);
    }
    
    
    
   
}
