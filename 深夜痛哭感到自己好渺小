#include <stdio.h>

void main()
{
    int week = 5;
    switch (week)
    {
            case 1:
            printf("星期一");
            break;
        case 2:
            printf("星期二");
            break;
        case 3:
            printf("星期三");
            break;
        case 4:
            printf("星期四");
            break;
        case 5:
            printf("星期五");
            break;
        case 6:
            printf("星期六");
            break;
        case 7:
            printf("星期日");
            break;
        default:
            printf("没有在深夜痛哭的人不足以语人生");
            break;
    }
}



          
    
