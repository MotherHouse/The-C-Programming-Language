#include <stdio.h>

int main()
{

    int x=0,y;
    scanf("%d",&x);
    
    if(x<1)
    {
        y=5*x;
    }
    else if(x>=1)
    
    
        y = (10*x-1)/3;
    
    
    
    printf("%d\n",y);
    return 0;
}
