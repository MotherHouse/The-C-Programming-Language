#include <stdio.h>
#include  "math.h"


int main()
{
    double a,b,c,d;
    int i;
    printf("please input your number:");
    scanf("%d",&a);
    scanf("%d",&b);
    scanf("%d",&c);
    switch (c)
    
case 1:
    d=a+b;
    break;
case 2:
    d=a-b;
    break;
case 3:
    d=a*b;
    break;
case 4:
    d=a/b;
    break;


    

    
    printf("%d\n",d);
}

