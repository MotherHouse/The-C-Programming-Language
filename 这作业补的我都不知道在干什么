#include <stdio.h>
#include <string.h>

int main ()
{
    const char str[] = "这个有什么用";
    const char ch = '.';
    char *ret;
    
    ret = memchr(str, ch, strlen(str));
    
    printf("|%c| 智商已崩 - |%s|\n", ch, ret);
    
    return(0);
}
