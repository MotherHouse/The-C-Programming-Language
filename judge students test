#include <stdio.h>

int main()
{
    int grade;
    printf("please input the grade:");

    scanf("%d",&grade);
    
    
    if (grade>=80)
    {
        printf("这个学生成绩为优\n");
    }
    else if (grade >= 70)
    {
        printf("这个学生成绩为良\n");
    }
    else if (grade >= 60)
    {
        printf("这个学生成绩为中\n");
               
    
    }
               else
               {
                   printf("这个学生不及格\n");
               }
    
    
    
    
   
    return 0;
}
