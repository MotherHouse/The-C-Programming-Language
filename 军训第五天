#include <stdio.h>
int winmine(int n)
{
    float y;
    if(n<=1)
        return 1;
    else
        y=n*winmine(n-1);
        
        
        return y;
    

void main()
    
{
    float n,end;
    scanf("%f",&n);
    end=winmine(n);
    
    printf("%f",end);
    
}
